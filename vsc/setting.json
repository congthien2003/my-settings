{
	"liveServer.settings.donotShowInfoMsg": true,
	"[python]": {
		"editor.formatOnType": true
	},
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 100,
	"redhat.telemetry.enabled": true,
	"prettier.bracketSameLine": true,
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.useEditorConfig": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"diffEditor.ignoreTrimWhitespace": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	"git.enableSmartCommit": true,
	"editor.fontFamily": "JetBrainsMonoNL Nerd Font Mono, FiraCode Nerd Font, JetBrains Mono, Fira Code",
	"editor.fontSize": 14,
	"editor.fontWeight": "400",
	"editor.suggestFontSize": 14,
	"editor.suggestLineHeight": 30,
	"terminal.integrated.lineHeight": 1.2,
	"editor.cursorBlinking": "smooth",
	"workbench.iconTheme": "material-icon-theme",
	"tabnine.experimentalAutoImports": false,
	"editor.minimap.enabled": false,
	"terminal.integrated.allowedLinkSchemes": [
		"file",
		"http",
		"https",
		"mailto",
		"vscode",
		"vscode-insiders",
		"docker-desktop"
	],
	"workbench.editor.highlightModifiedTabs": true,
	"chat.commandCenter.enabled": false,
	"workbench.layoutControl.enabled": false,
	"workbench.layoutControl.type": "menu",
	"terminal.integrated.fontFamily": "JetBrainsMonoNL Nerd Font Mono, FiraCode Nerd Font",
	"terminal.integrated.fontSize": 14,
	"terminal.integrated.fontWeight": "300",
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"terminal.integrated.drawBoldTextInBrightColors": true,
	"[csharp]": {
		"editor.defaultFormatter": "ms-dotnettools.csharp"
	},
	"editor.tabCompletion": "on",
	"database-client.autoSync": true,
	"workbench.tree.indent": 10,
	"workbench.tree.renderIndentGuides": "none",
	"window.zoomLevel": 0.55,
	"files.autoSave": "onWindowChange",
	"workbench.editor.showTabs": "none",
	"workbench.editor.alwaysShowEditorActions": true,
	"workbench.editor.labelFormat": "long",
	"workbench.editor.editorActionsLocation": "hidden",
	"window.titleBarStyle": "custom",
	"breadcrumbs.enabled": false,
	"editor.cursorStyle": "line",
	"editor.cursorWidth": 2,
	"editor.lineHeight": 24,
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.vertical": "hidden",
	"workbench.navigationControl.enabled": false,
	"editor.bracketPairColorization.enabled": true,
	"editor.lineNumbers": "on",
	"workbench.settings.applyToAllProfiles": [],
	"accessibility.dimUnfocused.opacity": 1,
	"accessibility.dimUnfocused.enabled": true,
	"editor.inlineSuggest.fontFamily": "JetBrainsMonoNL Nerd Font Mono",
	"editor.stickyScroll.enabled": false,
	"editor.selectionHighlight": true,
	"window.menuBarVisibility": "compact",
	"time-tracker.shortTimeFormat": "h [hours and] m [minutes]",
	"workbench.tree.enableStickyScroll": false,
	"window.restoreFullscreen": true,
	"workbench.colorTheme": "Catppuccin Frapp√©",
	"editor.inlayHints.fontFamily": "FiraCode Nerd Font",
	"editor.inlayHints.fontSize": 14,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"go.inlayHints.functionTypeParameters": true,
	"go.inlayHints.compositeLiteralFields": true,
	"csharp.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
	"csharp.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
	"csharp.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
	"csharp.inlayHints.enableInlayHintsForTypes": true,
	"dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"terminal.integrated.gpuAcceleration": "off",
	"workbench.colorCustomizations": {
		"terminal.background": "#00000000"
	},
	"terminal.integrated.suggest.enabled": true
}
